#Type Casting in Python
int_var =  int_var + 10
int_var
casted_int_var = str(int_var)
casted_int_var
print('int to str type casting converted :', casted_int_var)
casted_int_var1 = float(int_var)
casted_int_var1
print('int to str type casting converted :', casted_int_var1)
numeric_str = '123'
numeric_str1 = numeric_str + '50'
numeric_str1
print('numeric to str type casting converted:', numeric_str1)
name = input()
age = input()
